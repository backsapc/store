<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.CheckoutEntity" table="CHECKOUT" schema="PUBLIC" catalog="PUBLIC">
        <id name="id">
            <column name="ID" sql-type="integer(32)"/>
            <generator class="identity"/>
        </id>
        <property name="username">
            <column name="USERNAME" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
        <property name="date">
            <column name="DATE" sql-type="date(10)" not-null="true"/>
        </property>
        <property name="time">
            <column name="TIME" sql-type="time(8)" not-null="true"/>
        </property>
        <property name="address">
            <column name="ADDRESS" sql-type="varchar(8000)" length="8000" not-null="true"/>
        </property>
        <property name="deliveredby">
            <column name="DELIVEREDBY" sql-type="varchar(1000)" length="1000"/>
        </property>
        <property name="total">
            <column name="TOTAL" sql-type="double(64)" precision="64" not-null="true"/>
        </property>
        <property name="currency">
            <column name="CURRENCY" sql-type="varchar(5)" length="5" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>