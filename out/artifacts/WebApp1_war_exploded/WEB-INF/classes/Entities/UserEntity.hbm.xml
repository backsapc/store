<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.UserEntity" table="USER" schema="PUBLIC" catalog="PUBLIC">
        <id name="username">
            <column name="USERNAME" sql-type="varchar(60)" length="60"/>
        </id>
        <property name="defaulttab">
            <column name="DEFAULTTAB" sql-type="varchar(60)" length="60" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>